{
  "name": "AFKPlus",
  "productName": "AFKPlus",
  "version": "1.0.0",
  "description": "AFK+ Better minecraft AFK client for pro players.",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"npm:vite\" \"npm:electron:dev\"",
    "vite": "vite",
    "electron:dev": "wait-on tcp:5173 && cross-env IS_DEV=true electron .",
    "build": "vite build && electron-builder",
    "preview": "vite preview"
  },
  "author": "",
  "license": "ISC",
  "keywords": [],
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^31.0.1",
    "electron-builder": "^24.13.3",
    "electron-is-dev": "^3.0.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "vite": "^5.3.1",
    "wait-on": "^9.0.3"
  },
  "dependencies": {
    "mineflayer": "^4.33.0",
    "mineflayer-pathfinder": "^2.4.5",
    "minecraft-data": "^3.6.0",
    "prismarine-auth": "^2.7.0",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-toastify": "^9.1.3"
  },
  "build": {
    "appId": "com.afkplus.desktop",
    "productName": "AFKPlus",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    }
  }
}
